{
  "name": "layaair-export",
  "version": "1.0.0",
  "description": "export LayaAir project to H5 Game or WeChat game or Facebook game project.",
  "author": "liangmayong",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "export-fb": "node ./bin/export.js --platform facebook --input ./test/bin/ --output ./build/ --min",
    "export-h5": "node ./bin/export.js --platform h5 --input ./test/bin/ --output ./build/ --min",
    "export-wx": "node ./bin/export.js --platform wechat --input ./test/bin/ --output ./build/ --min --env prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoopschat/layaair-export.git"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "layaair-export": "./bin/export.js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "del": "^3.0.0",
    "commander": "2.15.1",
    "gulp": ">=4.0.0",
    "gulp-zip": "4.2.0",
    "gulp-cheerio": "^0.6.3",
    "gulp-concat": "^2.6.1",
    "gulp-downloader": "^1.0.4",
    "gulp-pngquant": "^1.0.12",
    "gulp-uglify": "^3.0.1",
    "gulp-replace": "^1.0.0",
    "minimist": "^1.2.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.2",
    "file-saver": "^1.3.3",
    "imports-loader": "^0.7.1",
    "string-replace-loader": "^2.1.1",
    "postcss-loader": "^2.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "keywords": [
    "layaair",
    "layaair-export"
  ]
}
